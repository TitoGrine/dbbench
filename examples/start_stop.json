{
	"duration": "10s",
	"setup": {
		"queries": [
			"drop table if exists mytable",
			"create table mytable (id int(7))"
		]
	},
	"teardown": {
		"queries": [
			"drop table mytable"
		]
	},
	"jobs": {
		"insert": {
			"queries": [
				"insert into mytable values (1)"
			],
			"concurrency": 2,
			"stop": "4s"
		},
		"clear": {
			"queries": [
				"insert into mytable values (1)"
			],
			"start": "5s",
			"count": 1
		},
		"insert batch": {
			"queries": [
				"insert into mytable values (1), (2), (3), (4), (5)"
			],
			"rate": 100,
			"start": "6s"
		}
	} 
}
